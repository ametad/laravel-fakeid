{
  "name": "oanhnn/laravel-fakeid",
  "type": "library",
  "description": "Using fake id on URL in Laravel 5.5+ application",
  "keywords": [
    "laravel",
    "package",
    "fakeid",
    "hashids"
  ],
  "homepage": "https://github.com/oanhnn/laravel-fakeid",
  "license": "MIT",
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "mockery/mockery": "^0.9",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "~6.1 || ~7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "suggest": {
    "hashids/hashids": "For using hashids driver (version >= 3.0.0)",
    "jenssegers/optimus": "For using optimus driver (version >= 0.2.3)"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\FakeId\\FakeIdServiceProvider"
      ],
      "aliases": {
        "FakeId": "Laravel\\FakeId\\Facades\\FakeId"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Laravel\\FakeId\\": "src/"
    }
  },
  "scripts": {
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html storage/coverage"
  },
  "support": {
    "issues": "https://github.com/oanhnn/laravel-fakeid/issues",
    "source": "https://github.com/oanhnn/laravel-fakeid"
  }
}
